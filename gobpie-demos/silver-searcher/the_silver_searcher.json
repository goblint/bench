{
  "files": [
    "the_silver_searcher"
  ],
  "allglobs": true,
  "ana": {
    "activated": [
      "expRelation",
      "base",
      "threadid",
      "threadflag",
      "threadreturn",
      "threadJoins",
      "escape",
      "mutexEvents",
      "mutex",
      "access",
      "race",
      "mallocWrapper",
      "mhp",
      "mallocFresh",
      "expsplit",
      "region"
    ],
    "base": {
      "privatization": "none",
      "context": {
        "non-ptr": false
      }
    },
    "thread": {
      "domain": "plain",
      "include-node": false
    },
    "malloc": {
      "wrappers": [
        "ag_malloc",
        "ag_realloc",
        "ag_calloc"
      ]
    },
    "race": {
      "free": false
    }
  },
  "sem": {
    "unknown_function": {
      "spawn": true,
      "invalidate": {
        "globals": false
      }
    }
  },
  "exp": {
    "earlyglobs": true
  },
  "cil": {
    "merge": {
      "inlines": true
    }
  },
  "dbg": {
    "timing": {
      "enabled": true
    }
  },
  "pre": {
    "keep": true,
    "cppflags": ["-D_FORTIFY_SOURCE=0"]
  },
  "lib": {
    "activated": [
      "c",
      "posix",
      "pthread",
      "gcc",
      "glibc",
      "linux-userspace",
      "goblint",
      "ncurses",
      "pcre",
      "zlib",
      "liblzma"
    ]
  },
  "warn": {
    "assert": false,
    "behavior": false,
    "integer": false,
    "cast": false,
    "race": true,
    "deadcode": false,
    "analyzer": false,
    "unsound": false,
    "program": false,
    "imprecise": false,
    "unknown": false,
    "error": false,
    "warning": true,
    "info": true,
    "debug": false,
    "success": false
  }
}
