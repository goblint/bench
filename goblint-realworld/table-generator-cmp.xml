<?xml version="1.0" ?>
<!DOCTYPE table PUBLIC "+//IDN sosy-lab.org//DTD BenchExec table 1.10//EN" "https://www.sosy-lab.org/benchexec/table-1.10.dtd">
<table>

  <union title="before">
    <result filename="../realworld-1/goblint.*.results.*.xml.bz2"/>

    <column title="status"/>
    <column title="cputime"/>
    <column title="walltime"/>
    <column title="memory" sourceUnit="B" displayUnit="MB"/>

    <!-- <column title="solvetime" sourceUnit="s" displayUnit="s">solving +([0-9.]+) ?s</column> -->
    <column title="vars">vars = (\d+)</column>
    <column title="evals">evals = (\d+)</column>
    <column title="live" relevantForDiff="true">live: (\d+)</column>
    <column title="safe" relevantForDiff="true">  safe: (\d+)</column>
    <column title="vulnerable" relevantForDiff="true">  vulnerable: (\d+)</column>
    <column title="unsafe" relevantForDiff="true">  unsafe: (\d+)</column>
  </union>

  <union title="after">
    <result filename="goblint.*.results.*.xml.bz2"/>

    <column title="status"/>
    <column title="cputime"/>
    <column title="walltime"/>
    <column title="memory" sourceUnit="B" displayUnit="MB"/>

    <!-- <column title="solvetime" sourceUnit="s" displayUnit="s">solving +([0-9.]+) ?s</column> -->
    <column title="vars">vars = (\d+)</column>
    <column title="evals">evals = (\d+)</column>
    <column title="live" relevantForDiff="true">live: (\d+)</column>
    <column title="safe" relevantForDiff="true">  safe: (\d+)</column>
    <column title="vulnerable" relevantForDiff="true">  vulnerable: (\d+)</column>
    <column title="unsafe" relevantForDiff="true">  unsafe: (\d+)</column>
  </union>

</table>
