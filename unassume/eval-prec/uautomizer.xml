<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.9//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.9.dtd">
<benchmark tool="ultimateautomizer" timelimit="900 s" hardtimelimit="960 s" memlimit="4 GB" cpuCores="1">

  <resultfiles>**/*.graphml</resultfiles>
  <option name="--full-output"/>

  <rundefinition name="uautomizer">
  </rundefinition>

  <tasks name="Prec">
    <includesfile>./Prec.set</includesfile>
    <propertyfile expectedverdict="true">./properties/unreach-call.prp</propertyfile>
  </tasks>

</benchmark>
